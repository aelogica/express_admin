content_for(:content) {
  main(role: "main").send("main-container") {
    flash_message_component
    if content_for?(:page_header)
      row.send("page-header") {
        h1(yield(:page_header))
      }
    end
    row.send("page-body") {
      yield
    }

  }
}

content_for(:page_css) {
  stylesheet_link_tag 'app_express/application'
}

render(template: 'layouts/express_admin/admin')
